  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FastEndpoints" Version="4.1.0-beta4" />
    <PackageReference Include="FastEndpoints.Swagger" Version="4.1.0-beta4" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.14.0" />
  </ItemGroup>

</Project>
